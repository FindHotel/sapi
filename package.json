{
  "name": "@findhotel/sapi",
  "version": "1.0.0",
  "description": "FindHotel Search API",
  "main": "dist/index.js",
  "files": [
    "dist",
    "types"
  ],
  "types": "dist/types/packages/core/src",
  "scripts": {
    "clean:dist": "rm -rf dist",
    "build": "npm run clean:dist && NODE_ENV=production rollup -c && npm run typescript:declarations",
    "lerna": "lerna",
    "lerna:bootstrap": "lerna bootstrap",
    "test": "ts-jest packages/*",
    "prepare": "npm run build",
    "rollup": "rollup -c",
    "rollup:watch": "rollup -c --watch",
    "typescript": "tsc",
    "typescript:declarations": "tsc --outDir dist/lib --declarationDir dist/types --declaration true --emitDeclarationOnly",
    "typescript:watch": "tsc --watch",
    "install:all": "lerna bootstrap",
    "playground": "cd packages/playground && npm run dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FindHotel/sapi.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FindHotel/sapi/issues"
  },
  "homepage": "https://github.com/FindHotel/sapi#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "eslint-config-xo-react": "^0.23.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "rollup": "^2.33.3",
    "rollup-plugin-terser": "^7.0.2",
    "ts-jest": "^26.4.4",
    "tslib": "^2.0.3",
    "typescript": "^4.1.2",
    "xo": "^0.35.0"
  }
}